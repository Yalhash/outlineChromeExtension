{
    "manifest_version": 2,
    "name": "GetArticle",
    "version": "1.0.0",
    "description": "Get past paywalls by going to outline!",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "no pay"
    },
    "permissions": [
        "tabs"
    ]
}

